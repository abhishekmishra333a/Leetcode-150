class Solution {
    public boolean containsNearbyDuplicate(int[] nums, int k) {

      HashMap<Integer,Integer> map=new HashMap<>();
      for(int i=0;i<nums.length;i++){
        int a=nums[i];
        if(map.containsKey(a)){
            int q=map.get(a);
            System.out.println(q);
            System.out.println(i);
            if(i-q<=k){
                return true;
            }
        }
        map.put(a,i);
      }
      return false;

    }
}
