class Solution {
    public boolean isAnagram(String s, String t) {
        if(s.length() != t.length())
            return false;
        int[] myMap = new int[26];
        for(int i = 0; i < s.length(); i++)
            myMap[s.charAt(i) - 'a']++;
        for(int i = 0; i < t.length(); i++)
            myMap[t.charAt(i) - 'a']--;
        for(int i = 0; i < myMap.length; i++){
            if(myMap[i] != 0)
                return false;
        }
        return true;
    }
}
